        <name>SI_getVehicleType</name>
        <script><![CDATA[var SI_getVehicleType = Class.create();

SI_getVehicleType.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	vehicleType: function() {
		var veh_Id = this.getParameter('sysparm_vehicleId');
				
		var gr = new GlideRecord('x_458834_car_reser_vehicles');
		if (gr.get(veh_Id)) {
			return gr.getValue('vehicle_type');
		}
		return ""; //Not found!
		
	},
	
    type: 'SI_getVehicleType'
});]]></script>
